<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.geneseeq.common.mysqldao.express_order.InternalExpress_OrderDao">

    <!-- 插入mysql express_order -->
    <insert id="insertExpress_Order" parameterType="com.geneseeq.common.pojo.express_order.SampleOrderExpressVO">
		insert into
		sample_order_express_t(order_id,express_id,remarks,created_by,created_at,updated_by,updated_at)
		values(#{order_id},#{express_id},#{remarks},#{created_by},#{created_at},#{updated_by},#{updated_at})
	</insert>

</mapper>
